{
    "name": "drupal-composer/drupal-project",
    "description": "Project template for Drupal 9 projects with Composer",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "",
            "role": ""
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "package",
            "package": {
                "name": "fancyapps/fancybox",
                "type": "drupal-library",
                "version": "4.1.2",
                "dist": {
                    "url": "https://github.com/fancyapps/ui/archive/refs/heads/main.zip",
                    "type": "zip"
                }
            }
        }
    ],
    "require": {
        "php": ">=7.4",
        "composer/installers": "^1.9",
        "cweagans/composer-patches": "^1.7",
        "drupal/admin_toolbar": "^3.1",
        "drupal/block_class": "^1.3",
        "drupal/block_field": "^1.0@RC",
        "drupal/bootstrap": "^3.23",
        "drupal/color_field": "^2.5",
        "drupal/commerce": "^2.29",
        "drupal/core-composer-scaffold": "^9.3",
        "drupal/core-recommended": "^9.3",
        "drupal/ctools": "^3.7",
        "drupal/devel": "^4.1",
        "drupal/devel_kint_extras": "^1.0",
        "drupal/easy_breadcrumb": "^2.0",
        "drupal/entity_browser": "^2.6",
        "drupal/menus_attribute": "^1.2",
        "drupal/module_filter": "^3.2",
        "drupal/paragraphs": "^1.13",
        "drupal/starrating": "^4.1@alpha",
        "drupal/themable_forms": "^1.0@beta",
        "drupal/twig_xdebug": "^1.2",
        "drupal/webform": "^6.1",
        "drupal/widget_engine": "^1.4",
        "drush/drush": "^10.6",
        "fancyapps/fancybox": "^4.1",
        "nolimits4web/swiper": "^4.5",
        "vlucas/phpdotenv": "^5.1",
        "webflo/drupal-finder": "^1.2"
    },
    "require-dev": {
        "drupal/core-dev": "^9.3"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "discard-changes": true,
        "sort-packages": true,
        "allow-plugins": {
            "composer/installers": true,
            "cweagans/composer-patches": true,
            "drupal/core-composer-scaffold": true
        }
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ],
        "files": ["load.environment.php"]
    },
    "scripts": {
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            }
        },
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/Commands/contrib/{$name}": ["type:drupal-drush"]
        },
        "composer-exit-on-patch-failure": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "patches": {
            "drupal/core": {
                "views theme suggestions": "https://www.drupal.org/files/issues/2923634-22.patch"
            }
        }
    }
}
